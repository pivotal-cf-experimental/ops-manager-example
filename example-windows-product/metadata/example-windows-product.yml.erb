<% product_name = "example-windows-product #{suffix}".parameterize -%>
---
name: <%= product_name %>
product_version: <%= version.inspect %>
minimum_version_for_upgrade: '0'
label: 'Ops Manager: Windows Example<%= suffix ? " #{suffix}" : nil%>'
rank: 2
metadata_version: '3.0'
description: An example windows product
service_broker: false
opsmanager_syslog: true
stemcell_criteria:
  os: <%= stemcell_os.inspect %>
  version: <%= stemcell_version.inspect %>
  enable_patch_security_updates: true
serial: true
releases:
- name: windows-utilities
  version: 0.12.0
  file: windows-utilities-0.12.0.tgz
- name: sample-windows-bosh-release
  version: 0.0.2
  file: sample-windows-bosh-release-0.0.2.tgz
form_types:
- property_inputs:
  - reference: ".no_op.log_string"
    label: Random String
    description: 'Enter random string'
  name: log-string
  label: Log String
  description: 'Logs a string'
job_types:
- name: no_op
  resource_label: No Operation
  max_in_flight: 100%
  single_az_only: false
  errand: false
  run_pre_delete_errand_default: true
  run_post_deploy_errand_default: true
  templates:
  - name: enable_ssh
    release: windows-utilities
  - name: log-string
    release: sample-windows-bosh-release
    manifest: |
      log-string:
        random_string: ((.no_op.log_string.value))
  instance_definition:
    default: 1
    configurable: true
  resource_definitions:
  - name: ram
    default: 1024
    configurable: true
  - name: ephemeral_disk
    default: 102400
    configurable: true
  - name: persistent_disk
    default: 0
    configurable: false
  - name: cpu
    default: 1
    configurable: true
  property_blueprints:
  - name: log_string
    type: string
    configurable: true
    optional: false
    freeze_on_deploy: false
    unique: false
icon_image: 
